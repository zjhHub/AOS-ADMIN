<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="engineQuery">
	<select id="selectByOrderId" parameterType="Dto" resultType="Fc_chargeback_order_sendPO">
		SELECT
			*
		FROM fc_chargeback_order_send
		where chargeback_order_id_ = #{chargeback_order_id_}
		<if test="orderparam != null">
			order by ${orderparam}
		</if>
	</select>

</mapper>