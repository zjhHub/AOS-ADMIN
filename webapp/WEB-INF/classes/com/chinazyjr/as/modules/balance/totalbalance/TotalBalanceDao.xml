<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="TotalBalance">

	<select id="queryTotalBalanceList" parameterType="Dto" resultType="Dto">
		SELECT
		<include refid="com.chinazyjr.as.dao.As_accountsDao.column" />
		FROM
			as_accounts
		WHERE
		    customer_id_ = #{customer_id_}
	</select>




</mapper>